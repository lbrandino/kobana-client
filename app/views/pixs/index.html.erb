<h1>Pixs</h1>
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Nome</th>
      <th>Documento</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @pixs.each do |pix| %>
      <tr>
        <td><%= pix.id %></td>
        <td><%= pix.name %></td>
        <td><%= pix.document_number %></td>
        <td><%= pix.status %></td>
        <td><%= link_to 'Exibir', pix_path(pix.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'Novo Pix', new_pix_path %>
